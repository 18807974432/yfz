<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ht.vo.Ticket" table="Ticket" >
        <id name="ticketid" column="ticketid" type="int">
            <generator class="native"/>
        </id>
        <property name="ticketNo" column="ticketNo" type="string"/>
        <property name="ticketType" column="ticketType" type="string"/>
        <property name="usePerson" column="usePerson" type="string"/>
        <property name="useTime" column="useTime" type="timestamp"/>
        <property name="startNo" column="startNo" type="string"/>
        <property name="endNo" column="endNo" type="string"/>
        <property name="prefix" column="prefix" type="string"/>
        <property name="invalid" column="invalid" type="string"/>
        <property name="buyTime" column="buyTime" type="timestamp"/>
        <property name="isAudit" column="isAudit" type="string"/>
        <property name="isEnd" column="isEnd" type="string"/>
        <property name="demo" column="demo" type="string"/>
        <property name="nextNo" column="nextNo" type="int"/>


    </class>
</hibernate-mapping>