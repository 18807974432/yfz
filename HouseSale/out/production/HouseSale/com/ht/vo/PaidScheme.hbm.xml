<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ht.vo.PaidScheme" table="PaidScheme">
        <id name="psid" column="psid">
            <generator class="native"/>
        </id>
        <property name="psname" column="psname"/>
        <property name="projectid" column="projectid"/>
        <property name="isBank" column="isBank"/>
        <property name="pstime" column="pstime"/>
        <property name="userid" column="userid"/>
    </class>
</hibernate-mapping>