<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ht.vo.JobsVo" table="Jobs">
		<id name="jobId">
			<column name="jobId"></column>
			<generator class="native"></generator>
		</id>
		<property name="jobType" type="string"/>
		<property name="jobName" type="string"/>
		<property name="day" type="int"/>
		<property name="money" type="float"/>
		<property name="jobDate" type="timestamp"/>
		<property name="processFlag" type="int"/>
		<property name="userId" type="string"/>
		<property name="remark" type="string"/>
	</class>
</hibernate-mapping>